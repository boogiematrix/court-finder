{{#each court}}
<div class="container-sm container-fluid mt-5" style="width: 100%;">
  <div class="row">
        <div class="card" style="width: 100%;">

    <div class="card-body">
        <h5 class="card-title">Court Address</h5>
        <a href='courts/{{id}}'>{{format_address address}}</a>
        <p><a href="http://www.google.com/maps/place/{{address}}">Google maps</a></p>
              
        <button type="button" class="btn btn-primary">
  Scheduled Games: <span class="badge bg-secondary">{{games.length}}</span>
</button></div>
    </div>
</div>
        </div>
        {{/each}}


